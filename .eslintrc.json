{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "settings": {
    "react": {
      "  version": "detect"
    }
  },
  "extends": [
    "plugin:react/recommended",
    "eslint:recommended"
    // "plugin:prettier/recommended",
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-var": "error",
    "require-await": "error",
    "eqeqeq": "error",
    "no-unused-vars": 1,
    "react/prop-types": "off",
    "no-undef": "off"
  }
}
